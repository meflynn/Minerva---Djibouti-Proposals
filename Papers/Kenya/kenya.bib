@article{palmer1990c,
	annote = {Main conclusions: (1) Allied states increase spending on defense when the US does, but to a lesser degree. (2) Small allied states DO NOT increase spending on defense when there are increased threats in the international environment. (3) There is a tradeoff between defense spending and domestic spending in small allied states but not large allied states. (4) Right governments in small allied states spend more on defense. However, there is no relationship between ideology and defense spending in large allied states. Of note from the article: They test three hypotheses: Allied states free ride of US defense spending. Allied states increase spending on defense when the US does, but to a lesser degree. Allied states increase spending on defense to the same degree as the US- they do this not because they're cooperating with the US but they see the same increase in threat in the international environment as the US. Their threat variable: ``The variable used to measure the level of international tension for the years  1949-78  is  derived from  the  Conflict  and  Peace  Data  Bank (COPDAB), as  developed  by  Edward  Azar.  The degree  of  conflict  or cooperation between the U.S. and the Soviet Union  for each year is used as  an  index  of the tension  between  East  and West. The tension variable for each year is (Con - Coop) / (Con  +  Coop) where  ``Con'' is the  number of conflictual acts (coded  9 through  15) and ``Coop'' is the number of cooperative acts (1 through 7 in COPDAB) that took place between the two states. (Acts coded ``8'' in the COPDAB data were excluded from analysis.)  The index thus can vary from + 1.00 (for all conflictual acts) to - 1.00 (for all cooperative acts). The international tension measure for 1979-1984 is taken from Kegley and Wittkopf (1987:  57).''},
	author = {Palmer, Glenn},
	date-modified = {2012-10-02 02:45:52 +0000},
	journal = {Political Research Quarterly},
	number = {3},
	pages = {479},
	publisher = {Sage Publications},
	title = {NATO, social and defense spending, and coalitions},
	volume = {43},
	year = {1990}}

@book{MorganPalmer2006,
	annote = {States pursue two things- change and maintenance. Foreign policy portfolios- like guns vs butter, except guns vs. aid. Perhaps we can assume that if a host country accepts US troops, they either want to change the SQ- increase security in their state- increase military spending...or maintain the security SQ and then change the SQ on another foreign policy area. Assumes that the more resources a state has, the more they can spend on foreign policy goods. Maybe start out by saying- We assume the US places troops in other countries for security purposes. We assume that host states accept the troops for one of three reasons- to increase security, to reallocate resources to other foreign policy areas, or to reallocate resources to other domestic policy areas. States will engage in more change seeking and more maintenance seeking behavior as their resources increase. pg. 35 Joining an alliance allows a state to engage in more change or maintenance seeking policies. If the alliance provides maintenance, the state will allocated resources away from defense (a maintenance policy) and more towards change seeking policies (foreign aid)- and vice versa if it is an alliance that promotes change. pgs. 41-42. Results suggest that when states join alliances, they increase military spending and increase their military capabilities.},
	author = {Morgan, T. Clifton and Glenn Palmer},
	date-added = {2011-06-28 14:54:43 -0400},
	date-modified = {2011-06-28 14:55:24 -0400},
	publisher = {Princeton: Princeton University Press},
	title = {A Theory of Foreign Policy},
	year = {2006}}

